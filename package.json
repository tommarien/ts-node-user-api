  "bugs": {
    "url": "https://github.com/tommarien/ts-node-user-api/issues"
  },
  "homepage": "https://github.com/tommarien/ts-node-user-api#readme",
  "devDependencies": {
    "@types/boom": "^7.1.1",
    "@types/bunyan": "^1.8.4",
    "@types/express": "^4.11.1",
    "@types/joi": "^13.0.5",
    "@types/mocha": "^2.2.48",
    "@types/mongoose": "^5.0.2",
    "@types/node": "^8.9.4",
    "@types/supertest": "^2.0.4",
    "nodemon": "^1.14.12",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.1",
    "tslint-config-airbnb": "^5.6.0",
    "typescript": "^2.7.1"
  },
  "dependencies": {
    "boom": "^7.1.1",
    "bunyan": "^1.8.12",
    "chai": "^4.1.2",
    "express": "^4.16.2",
    "express-request-id": "^1.4.0",
    "joi": "^13.1.2",
    "migrate-mongo": "^2.2.1",
    "mocha": "^5.0.1",
    "mongoose": "^5.0.6",
    "object-mapper": "^5.0.0",
    "supertest": "^3.0.0"
  }
